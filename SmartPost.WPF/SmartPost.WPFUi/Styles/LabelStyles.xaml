    <!--Label My Name-->
    <Style x:Key="MyName" TargetType="{x:Type Label}">
        <!--//<Setter Property="Foreground" Value="{DynamicResource lblMastersNameColor}"/>-->
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="FontFamily" Value="JetBrains Mono"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="FontSize" Value="18"/>
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="Margin" Value="0 20 0 0"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="Content" Value="Bek"/>
    </Style>





    <Style x:Key="OnlineMaster" TargetType="{x:Type Label}">
        <Setter Property="Foreground" Value="#bcb8b1"/>
        <Setter Property="FontFamily" Value="JetBrains Mono"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="Height" Value="20"/>
        <Setter Property="Height" Value="50"/>
        <Setter Property="VerticalAlignment" Value="Bottom"/>
        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="Content" Value="Administrator"/>
        <Setter Property="Margin" Value="0 0 0 5"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Label}">
                    <Border x:Name="B" BorderThickness="0">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="20"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Path Grid.Column="0"
                              Data="{DynamicResource server}"
                              Fill="#bcb8b1"
                              Opacity="0.5" Height="12" Width="12"
                              Stretch="Uniform" HorizontalAlignment="Right" Margin="3"/>
                            <Label Grid.Column="1" Content="{TemplateBinding Content}" FontSize="{TemplateBinding FontSize}"
                               Foreground="{TemplateBinding Foreground}" FontFamily="{TemplateBinding FontFamily}"
                               Padding="{TemplateBinding Padding}" VerticalAlignment="Center"/>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>



    <Style x:Key="Myinfo" TargetType="{x:Type Label}">
        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Margin" Value="0 0 20 0"/>
        <Setter Property="Height" Value="70"/>
